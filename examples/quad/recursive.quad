enum Node:
    Ende
    Link(int, Addr<Node>)

func main() -> int:
    cell tail: Addr<Node> := alloc(Node::Ende)
    cell head: Node := Node::Link(10, tail)

    case head:
        when Node::Link(val, next_ref):
            println(val)
        when Node::Ende:
            println(0)

    dealloc(tail)
    back 0
