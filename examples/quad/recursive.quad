enum Node:
    Ende
    Link(int, ref<Node>)

func main() -> int:
    cell tail: ref<Node> := heap(Node::Ende)
    cell head: Node := Node::Link(10, tail)

    case head:
        when Node::Link(val, next_ref):
            println(val)
        when Node::Ende:
            println(0)

    free(tail)
    back 0
