type User {
    name: text,
    age: intg
}

impl User:
    func make(n: text, a: intg) -> User:
        vars u: User := User{ name: n, age: a }
        back u

    func grow(self) -> void:
        self.age := self.age + 1

func main() -> intg:
    vars u: User := User.make("anna", 20)
    u.grow()
    u.grow()
    echo(u.age)
    back u.age
