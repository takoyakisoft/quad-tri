func atoi(s: text) -> Result<int, text>:
    cell p: Addr<int> := alloc(0)
    when atoi_native(s, p):
        bind val: int := deref(p)
        dealloc(p)
        back Result<int, text>::Ok(val)
    else:
        dealloc(p)
        back Result<int, text>::Err("invalid number")

func itoa(n: int) -> text:
    back itoa_native(n)
